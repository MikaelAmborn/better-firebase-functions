{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "max-len": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": ["warn", { "devDependencies": true }],
    "no-console": 0
  }
}
